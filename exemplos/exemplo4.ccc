def linearSearch(int arr, int n, int x)
{
    int i;
    int ret;
    for (i = 0; i < n; i = i + 1) {
        if (arr[i] == x) {
            print "return i";
            ret = i;
            return;
        }
    }
    print "return 0";
    ret = 0;
    return;
}

def binarySearch(int arr, int l, int r, int x)
{
    if (r >= l) {
        int mid;
        mid = l + (r - l) / 2;
        int temp;
 
        if (arr[mid] == x) {
            print "return mid";
            ret = mid;
            return;
        }
 
        if (arr[mid] > x) {
            print "return binarySearch(arr, l, mid - 1, x)";
            temp = mid - 1;
            ret = binarySearch(arr, l, temp, x);
            return;
        }

        print "return binarySearch(arr, mid + 1, r, x);";
        temp = mid +1;
        ret = binarySearch(arr, temp, r, x);
        return;
    }
    
    print "return -1";
    return;
}

