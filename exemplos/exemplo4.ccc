def execute_example_binarySearch() {
    int arr[5];
    arr[0] = 2;
    arr[1] = 3;
    arr[2] = 4;
    arr[3] = 10;
    arr[4] = 40;
    int n;
    int x;
    int size_arr;
    int size_arr0;
    int arr0;
    int result;
    int n_minus1;
    int zero;
    x = 10;
    size_arr = sizeof(arr);
    arr0 = arr[0];
    size_arr0 = sizeof(arr0);
    n = size_arr / size_arr0;
    n_minus1 = n-1;
    zero = 0;
    result  = binarySearch(arr, zero, n_minus1, x);
